# Server Configuration Environment Variables
# Copy this file to .env and update with your values

# Server Settings
SERVER_HOST=0.0.0.0
SERVER_PORT=8080
DEBUG=False

# CORS Settings (use * for all origins, or specific domains)
CORS_ORIGINS=*

# Database Path
DB_PATH=barangay.db

# DuckDNS Configuration (optional)
DUCKDNS_DOMAIN=your-domain.duckdns.org
DUCKDNS_TOKEN=your-duckdns-token

# Alternative: Multiple CORS origins
# CORS_ORIGINS=https://yourdomain.duckdns.org,https://localhost:8080,http://192.168.1.100:8080
